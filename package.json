{
    "name": "YukioUsuzumi",
    "displayName": "Yukio Usuzumi",
    "description": "",
    "version": "1.1.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/y-usuzumi/visual-studio-code-extension-YukioUsuzumi.git"
    },
    "publisher": "y-usuzumi",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "yu.emacs.centerToScreen",
                "title": "Yukio Usuzumi: Emacs: Center to Screen"
            },
            {
                "command": "yu.formatAndOrganizeImports",
                "title": "Yukio Usuzumi: Format and organize imports"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
